// Slider
$(function() {
	$("#transpslider").slider({
		range : "max",
		min : 0,
		max : 100,
		value : 0,
		slide : function(event, ui) {
			$("#txttransp").val(ui.value);
		}
	});
	$("#txttransp").val($("#transpslider").slider("value"));
});

