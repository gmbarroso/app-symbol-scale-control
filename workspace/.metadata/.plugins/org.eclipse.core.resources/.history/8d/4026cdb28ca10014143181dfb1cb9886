// Add new scale
$(function() {
	$("#addnewscale").click(
			function() {
				if (i < 4) {
					$("#rules").append(
							"<h3>Rule</h3><div style='height:auto;'>"
									+ $("#rulemaster").html() + "</div>");
					i++;
					if(i==4){
//						$("#addnewscale").disable();
					}
					if (i > 1) {
//						$(".cerrar").enable();
					}
					$("#rules").accordion("refresh");
					definenewrule();
				}
			});
});

